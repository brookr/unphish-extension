{
  "name": "Unphish Extension",
  "description": "Quick badging for identifying true sites within a community.",
  "homepage_url": "https://github.com/brookr/unphish-extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Unphish: Stay Safu!",
    "default_icon": {
      "16": "images/logo-16.png",
      "32": "images/logo-32.png",
      "48": "images/logo-48.png",
      "128": "images/logo-128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ]
}
